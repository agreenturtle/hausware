<% content_for :page_title do %>
setting Details - 
<% end %>
<div class="container">
<div class="page-header">
<h1>
<div class="pull-left">
<%= link_to "<span class=\"glyphicon glyphicon-step-backward\" aria-hidden=\"true\"></span>".html_safe, settings_path, class: "btn btn-default", title: "Back to settings" %>
&nbsp;
</div>
<div class="pull-right">
<%= link_to "<span class=\"glyphicon glyphicon-pencil\" aria-hidden=\"true\"></span> Edit".html_safe, edit_setting_path(@setting), class: "btn btn-warning" %>
</div>
setting Details
</h1>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Org</label>
  <div class="col-sm-9"><%= @setting.org %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Default Location Spaces</label>
  <div class="col-sm-9"><%= @setting.default_location_spaces %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Stripe publishable key</label>
  <div class="col-sm-9"><%= @setting.stripe_publishable_key %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Stripe secret key encrypted</label>
  <div class="col-sm-9"><%= @setting.stripe_secret_key_encrypted %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Salt</label>
  <div class="col-sm-9"><%= @setting.salt %></div>
</div>

</div>

